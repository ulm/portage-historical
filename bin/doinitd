# $Header: /local/data/ulm/cvs/history/var/cvsroot/gentoo-src/portage/bin/doinitd,v 1.1 2004/03/22 01:40:55 carpaski Exp $

if [ ${#} -lt 1 ] ; then
	echo "doinfo: at least one argument needed"
	exit 1 	
fi

tmp_EXEDESTTREE="${EXEDESTTREE}"
EXEDESTTREE=/etc/init.d/
doexe "$@"
EXEDESTTREE="${tmp_EXEDESTTREE}"
unset tmp_EXEDESTTREE
